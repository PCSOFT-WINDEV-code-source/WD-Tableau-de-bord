      // Résumé : Affiche le graphe
      // Syntaxe :
      // AfficheGraphe ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheGraphe()
      
      nValeur est un entier
      nTotal est un entier
      
      
      nValeur = Hasard(50,200)
      grAjouteDonnée(GRF_Stock, 1, 1, nValeur)
      nTotal += nValeur
      
      nValeur = Hasard(10,50)
      grAjouteDonnée(GRF_Stock, 2, 1, nValeur)
      nTotal += nValeur
      
      grDessine(GRF_Stock)
      
      
      // Alerte
      si nTotal <= 150 ALORS
      	IMG_EtatStock = IMG_EtatStock_ALERTE
      	LIB_EtatStock = "ALERTE"
      SINON // OK
      	IMG_EtatStock = IMG_EtatStock_ok
      	LIB_EtatStock = "OK"
      FIN
