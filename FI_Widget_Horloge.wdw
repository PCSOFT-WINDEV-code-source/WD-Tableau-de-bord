   -
     name : GMTAdd
     procedure_id : 1337407336571930720
     type_code : 14
     code : |1-
      // Résumé : Ajoute une ville
      // Syntaxe :
      //GMTAdd (<sVille> est chaîne, <sGMT> est chaîne)
      //
      // Paramètres :
      //	sVille (chaîne ANSI) : Nom de la ville
      //	sGMT (chaîne ANSI) : Décalage GMT
      // Valeur de retour :
      // 	Aucune
      //
      
      Procédure PRIVEE GMTAdd(LOCAL sVille est une chaine, LOCAL sGMT est une chaine)
      	
      // Mémorise le décalage pour la ville
      gtaListeVillesGMT[sVille] = sGMT
     type : 458752
   -
     name : CalculeHeure
     procedure_id : 1337408401723937394
     type_code : 14
     code : |1+
      // Résumé : Calcule l'heure en fonction du décalage
      // Syntaxe : 
      //[ <Résultat> = ] CalculeHeure (<hHeure>, <sDécalageGMT>)
      //
      // Paramètres :
      //	hHeure : Heure à décaler selon l'offset GMT
      //	sDécalageGMT : Offset GMT
      // Valeur de retour :
      // 	heure : Heure décalée
      
      Procédure PRIVEE CalculeHeure(hHeure, sDécalageGMT)
      
      hHeureCalcul est une heure  = hHeure
      sCodeGMT	 est une chaine = sDécalageGMT
      
      // Extrait le nombre d'heures de décalage
      nNbHeures	 est un entier  = sCodeGMT[[2 a 3]]
      
      // Extrait le nombre de minutes de décalage
      nNbMinutes   est un entier  = sCodeGMT[[4 a 5]]
      
      si sCodeGMT[[1]] = "+" alors
      	// <GMT +HHMM>
      	
      	// Ajoute le nombre de minutes et d'heures du décalage GMT
      	hHeureCalcul..minute += nNbMinutes
      	hHeureCalcul..heure  += nNbHeures
      	
      sinon
      	// <GMT -HHMM>
      	
      	// Soustrait le nombre de minutes et d'heures du décalage GMT
      	hHeureCalcul..minute -= nNbMinutes
      	hHeureCalcul..heure  -= nNbHeures
      	
      FIN
      
      // Renvoie la nouvelle heure
      renvoyer hHeureCalcul
      
     type : 458752
