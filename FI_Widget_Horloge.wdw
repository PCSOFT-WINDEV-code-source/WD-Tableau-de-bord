  procedures :
   -
     name : AfficheHeure
     procedure_id : 1337395405151445815
     type_code : 14
     code : |1+
      // Résumé : Affiche l'heure
      // Syntaxe :
      //AfficheHeure ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheHeure()
      
      sDecalageGMT est une chaîne
      hHeureActuelle est une heure
      nHeure, nMinute sont des entiers
      nCentreX, nCentreY sont des entiers
      nDiametreCercle est un entier
      nAngleHeure, nAngleMinute sont des entiers
      nX, nY sont des entiers
      MonImage est une image
      
      // Récupère le décalage GMT
      sDecalageGMT = gtaListeVillesGMT[gsVille]
      // Calcule le décalage
      hHeureActuelle = CalculeHeure(Maintenant(), "-0100")
      hHeureActuelle = CalculeHeure(hHeureActuelle, sDecalageGMT)
      // Récupère les heures et minutes
      nHeure = hHeureActuelle..Heure
      nMinute = hHeureActuelle..Minute
      // L'heure doit être passée en format 12:00
      SI nHeure > 12 ALORS nHeure -= 12
      
      // Calcule l'angle
      nAngleHeure  = (nHeure + (nMinute / 60 ))* (360 / 12) - 90 
      nAngleMinute = nMinute * (360 / 60) - 90
      
      // Début de dessin
      dDébutDessin(MonImage)
      // Dessin net
      dChangeMode(dessinAntiAliasing)
      MonImage..largeur = IMG_Dessin..largeur
      MonImage..Hauteur = IMG_Dessin..Hauteur
      
      // Centre
      nCentreX = MonImage..largeur/2
      nCentreY = MonImage..hauteur/2
      
      // Dessine le cercle de décor
      nDiametreCercle = Min(MonImage..Largeur, MonImage..Hauteur)
      nDiametreCercle -= 6
      dCercle(nCentreX - nDiametreCercle/2, nCentreY - nDiametreCercle/2, nCentreX + nDiametreCercle/2, nCentreY + nDiametreCercle/2, rvb(153,153,153))
      nDiametreCercle -= 12 
      dCercle(nCentreX - nDiametreCercle/2, nCentreY - nDiametreCercle/2, nCentreX + nDiametreCercle/2, nCentreY + nDiametreCercle/2, blanc)
      
      // Dessine l'aiguille des minutes
      nDiametreCercle -= 10
      // Calcule le point X,Y de la minute
      nX = nCentreX + (Cosinus(nAngleMinute) * (nCentreX- (nDiametreCercle/4)))
      nY = nCentreY + (sinus(nAngleMinute) * (nCentreY - (nDiametreCercle/4)))	
      // Dessine l'aiguille des minutes
      dLigne(nCentreX, nCentreY, nX, nY, rvb(71,133,186), 4)	
      
      // Dessine l'aiguille des heures
      nDiametreCercle -= 10
      // Calcule le point X,Y de l'heure
      nX = nCentreX + (Cosinus(nAngleHeure) * (nCentreX - (nDiametreCercle/2)))
      nY = nCentreY + (Sinus(nAngleHeure) * (nCentreY - (nDiametreCercle/2)))	
      // Dessine l'aiguille des heures
      dLigne(nCentreX, nCentreY, nX, nY, RVB(26,161,225), 4)	
      
      // Affiche le dessin
      IMG_Dessin = MonImage
