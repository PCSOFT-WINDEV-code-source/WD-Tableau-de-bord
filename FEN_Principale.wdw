     internal_properties : CAAAAAgAAADCDmUYQq7XnCH9TBhY0pz5PG7AY+j/HHYz2OZ7/LbTPEkvqRatxZdKrtOBO4TNm6WQE/455ulFjYRi9WBOORLbcnj1LeM+EEf3UQLkM4yBB5VL1umv7WhOGRu68gwfpbk6qmYfHuTUjlMnRncdUbqsCD/QQBSgtZiO4HaeEpt9AysO+wRJcs86yomCWvORLxECQ25T2IbR
     procedure_id : 1337435782142311906
     type_code : 14
     code : |1-
      // Résumé : Modifie de façon aléatoire les valeurs de test pour les widgets
      // Syntaxe :
      //ModifieValeursTest ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //// Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant 1 seconde entre chaque appel
      // Chaque appel suivant exécute une seule fois la procédure, sans timer
      // 
      
      PROCEDURE ModifieValeursTest()
      
      // Ajoute / enlève des emails
      gnNbEmails += abs((hasard(-5,5)))
      si gnNbEmails = 0 alors gnNbEmails = 1
      si gnNbEmails >= 100 alors gnNbEmails = 99
      
      
      // Ajoute / enlève des rappels
      gnNbRappels += abs((Hasard(-5,5)))
      SI gnNbRappels = 0 ALORS gnNbRappels = 1
      SI gnNbRappels >= 100 ALORS gnNbRappels = 99
